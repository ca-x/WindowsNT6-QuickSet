Func DllInstall($sDll)
	$aCall = DllCall($sDll, "long", "DllRegisterServer")
	If @error Or $aCall[0] Then
		Return False
	Else
		Return True
	EndIf
EndFunc   ;==>DllInstall
Func DllUnInstall($sDll)
