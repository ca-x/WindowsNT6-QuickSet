			If GUICtrlRead($SecuritySet[1]) = $GUI_CHECKED Then
				RegWrite('HKEY_LOCAL_MACHINE' & $OSFlag & '\SOFTWARE\Classes\.reg', '', 'REG_SZ', 'txtfile')
